@page
@addTagHelper *, Microsoft.AppNetCore.Mvc.TagHelpers

<div class="text-center">
    <h2>Thanks!</h2>
    <p>Thanks for placing order #@OrderId</p>
    <p>Your subscription is ready!</p>
    <a class="btn btn-primary" href="/">Return to Store</a>
</div>

@functions {
        [BindProperty(SupportsGet = true)]
        public string? OrderId { get; set; }
}
